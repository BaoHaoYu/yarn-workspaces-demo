    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "url": "^0.11.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "yargs-parser": "^13.1.1"
  }
}
