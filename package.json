{
  "name": "yarn-workspaces-demo",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:w": "rollup -c rollup.config.js -w",
    "tslint": "tslint --fix packages/**/*{.ts,.tsx} .storybook/*.ts stories/**/*{.ts,.tsx}",
    "stylelint": "stylelint --fix packages/**/*.scss",
    "release": "lerna publish --no-git-tag-version --no-push",
    "link": "lerna exec -- yarn link",
    "updated": "lerna updated",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint && npm run stylelint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.1.7",
    "@storybook/addon-info": "^5.1.7",
    "@storybook/addon-jest": "^5.1.8",
    "@storybook/addon-knobs": "^5.1.7",
    "@storybook/addon-links": "^5.1.7",
    "@storybook/addon-notes": "^5.1.7",
    "@storybook/addon-storysource": "^5.1.7",
    "@storybook/addons": "^5.1.7",
    "@storybook/react": "^5.1.7",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.15",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__addon-jest": "^3.4.3",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__react": "^4.0.2",
    "@types/webpack": "^4.4.32",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.0.1",
    "globby": "^9.2.0",
    "husky": "^2.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "lerna": "^3.15.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "precss": "^4.0.0",
    "react-hot-loader": "^4.11.0",
    "rollup": "^1.15.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.0.2",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.21.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "url": "^0.11.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0"
  }
}
