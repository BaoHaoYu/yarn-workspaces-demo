{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "prettier": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "include": [
    "packages",
    "webpack-ts",
    ".storybook",
    "stories"
  ],
  "exclude": [
    "node_modlues",
    "dist/"
  ]
}